{
  "name": "@cycle/time",
  "version": "0.8.0",
  "description": "A time driver designed to enable awesome testing and dev tooling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Nick Johnstone",
  "license": "MIT",
  "homepage": "https://github.com/cyclejs/time#readme",
  "files": [
    "dist/",
    "most.js",
    "rxjs.js",
    "most.d.ts",
    "rxjs.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "budo -d example example/index.ts:index.js --live -- -p tsify",
    "test": "npm run compile && npm run test/mocha && npm run test/jest && npm run test/tape && npm run test/ava && npm run test/docs",
    "test/mocha": "mocha 'test/mocha/**/*.ts' --compilers ts:ts-node/register",
    "test/jest": "jest test/jest",
    "test/tape": "tape test/tape/*.js",
    "test/ava": "ava test/ava",
    "test/auto": "mocha 'test/mocha/**/*.ts' --compilers ts:ts-node/register --watch -R min",
    "test/docs": "markdown-doctest",
    "compile": "tsc && tsc rxjs.ts most.ts --declaration --lib DOM,ES5,ES6",
    "prepublish": "npm run compile",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclejs/time.git"
  },
  "bugs": {
    "url": "https://github.com/cyclejs/time/issues"
  },
  "dependencies": {
    "@cycle/run": "^1.0.0-rc.9",
    "combine-errors": "^3.0.3",
    "raf": "^3.3.0",
    "setimmediate": "^1.0.5",
    "sorted-immutable-list": "^1.1.0",
    "variable-diff": "^1.1.0",
    "xstream": "*"
  },
  "devDependencies": {
    "@cycle/dom": "^15.0.0-rc.2",
    "@cycle/rxjs-run": "^4.0.0-rc.6",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.8",
    "ava": "^0.19.1",
    "browserify": "^14.3.0",
    "budo": "^10.0.2",
    "garnish": "^5.2.0",
    "greenkeeper-postpublish": "^1.0.1",
    "jest": "^19.0.2",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.4.1",
    "most": "^1.2.2",
    "rxjs": "^5.1.0",
    "snabbdom-selector": "^1.1.1",
    "tape": "^4.6.3",
    "ts-node": "^3.0.2",
    "tsify": "^3.0.0",
    "typescript": "^2.1.5"
  }
}
